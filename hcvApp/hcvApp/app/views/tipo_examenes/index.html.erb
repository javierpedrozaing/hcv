<p id="notice"><%= notice %></p>

<h1>Listing Tipo Examenes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Perfil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_examenes.each do |tipo_examene| %>
      <tr>
        <td><%= tipo_examene.nombre %></td>
        <td><%= tipo_examene.perfil %></td>
        <td><%= link_to 'Show', tipo_examene %></td>
        <td><%= link_to 'Edit', edit_tipo_examene_path(tipo_examene) %></td>
        <td><%= link_to 'Destroy', tipo_examene, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo examene', new_tipo_examene_path %>
